{
  "name": "pwa-sample",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng build --prod --base-href=\".\" --output-path=\"build\" && static -p 4200 ./build",
    "build": "rimraf build && ng build --base-href=\".\" --output-path=\"build\"",
    "build-prod": "rimraf build && ng build --prod --base-href=\".\" --output-path=\"build\"",
    "build-aot": "rimraf build && ng build --prod --aot=true --base-href=\".\" --output-path=\"build\"",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.2.2",
    "@angular/compiler": "^5.2.2",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@angular/router": "^5.2.2",
    "@types/detect-browser": "^2.0.0",
    "core-js": "^2.5.3",
    "detect-browser": "^2.0.0",
    "dexie": "^2.0.1",
    "font-awesome": "4.7.0",
    "include-media": "1.4.9",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.29",
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.2",
    "@angular/service-worker": "^5.2.2",
    "@types/node": "7.0.18",
    "codelyzer": "^4.0.2",
    "mkdirp": "0.5.1",
    "node-static": "^0.7.10",
    "rimraf": "2.6.1",
    "tslint": "~5.3.2",
    "typescript": "2.6.2"
  }
}
